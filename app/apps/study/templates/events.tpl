<div class="col-sm-8 col-sm-offset-2 calendar-input">
	<div class="panel panel-default">
		<div class="panel-heading input-header text-center"> <%= obj.dateString %> </div>
		<div class="panel-body">


			<ul class="js-events-list event-icons-wrapper col-sm-8 col-sm-offset-2 list-group"></ul>

			 <div class="input-group col-sm-8 col-sm-offset-2">
			      <input type="text" class="js-event-input event-title-input form-control" placeholder="Event">
			      <span class="input-group-btn">
			        <button class=" js-add-event add-event-btn glyphicon-plus" type="button"></button>
			      </span>
			  </div><!-- /input-group -->
			  <button type="button" class="col-sm-6 col-sm-offset-3 evt-clndr-return-btn js-save">
			  	<i class="glyphicon cal-icon glyphicon-calendar"></i>  Back to Calendar</button>

		</div>
	</div>
</div>




